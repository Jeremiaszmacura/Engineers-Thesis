  "main": "src/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node src/server.js",
    "dev-start": "nodemon src/server.js",
    "lint": "eslint src/server.js",
    "create-database": "cd src && sequelize db:create && cd ..",
    "drop-database": "cd src && sequelize db:drop && cd ..",
    "migration": "cd src && sequelize db:migrate && cd ..",
    "undo-latest-migration": "cd src && sequelize db:migrate:undo && cd ..",
    "undo-all-migrations": "cd src && sequelize db:migrate:undo:all && cd ..",
    "migration-status": "cd src && sequelize db:migrate:status && cd ..",
    "seed": "cd src && sequelize db:seed:all && cd ..",
    "undo-seed": "cd src && sequelize db:seed:undo:all && cd .."
  },
